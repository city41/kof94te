    {
      "type": "prom",
      "description": "Have the game stop playing any sound effects from team select.",
      "address": "106556",
      "subroutine": true,
      "patchAsm": [
        "cmpi.b #1, $IN_CHAR_SELECT_FLAG",
        "bne play ; not in char select, let the sound play",
        "cmpi.b #$MAIN_PHASE_CPU_SELECT, $MAIN_HACK_PHASE ; is this cpu select?",
        "beq play ; then play the sound",
        "cmpi.b #$MAIN_PHASE_SUBSEQUENT_SINGLE_PLAYER_SELECT, $MAIN_HACK_PHASE ; is this cpu subsequent select?",
        "beq play ; then play the sound",
        "cmpi.b #$61, D0 ; in char select, is the sound the chose team sound effect?",
        "bne checkForMovementSfx ; some other sound, keep going",
        "cmpi.b #1, $PLAY_HERE_COMES_CHALLENGER_CHIME ; but is it actually the here comes challenge chime (same sfx)?",
        "beq playHCC ; it is? then let it play",
        "bra done ; it's not? then it's a regular choice sfx, don't let it play",
        "checkForMovementSfx:",
        "cmpi.b #$60, D0 ; is it the cursor movement sfx?",
        "beq done ; then don't let it play",
        "bra play ; otherwise, let it play",
        "playHCC:",
        "move.b #0, $PLAY_HERE_COMES_CHALLENGER_CHIME ; turn off the flag, we have consumed it",
        "play:",
        "move.b D0, $320000",
        "done:",
        "rts"
      ]
